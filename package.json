  "scripts": {
    "_prepare": "husky install && husky add .husky/commit-msg && husky add .husky/post-commit",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:start": "pm2 start dist/src/main.js --name $npm_package_name --restart-delay=10000 --wait-ready --listen-timeout=30000",
    "pm2:stop": "(pm2 stop -s $npm_package_name || :) && (pm2 delete -s $npm_package_name || :)",
    "pm2:list": "pm2 list $npm_package_name",
    "pm2:logs": "pm2 logs $npm_package_name",
    "pm2:flush": "pm2 flush -s $npm_package_name ||Â :",
    "pm2:show": "pm2 show $npm_package_name",
    "pm2:monit": "pm2 monit $npm_package_name",
    "pm2:full-restart": "npm run pm2:stop && npm run pm2:flush && npm run pm2:start",
    "pm2:startup": "pm2 startup",
    "pm2:save": "pm2 save",
    "pm2:unstartup": "pm2 unstartup",
    "pm2:update": "pm2 update"
